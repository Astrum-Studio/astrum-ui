{
  "name": "astrum-ui",
  "version": "0.1.1",
  "description": "React UI компоненты Astrum Studio",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "style": "dist/styles.css",
  "scripts": {
    "build": "tsup && copyfiles \"src/**/*.css\" dist -u 1",
    "clean": "rimraf dist",
    "storybook": "npx storybook dev -p 3333 --host 0.0.0.0",
    "storybook:build": "npx storybook build -o storybook-static"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@storybook/test": "^8.4.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "copyfiles": "^2.4.1",
    "eslint-plugin-storybook": "10.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.6",
    "storybook": "^10.2.8",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.4.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Astrum-Studio/astrum-ui.git"
  }
}
